<!-- form handling -->
<form action="/api/warehouse" method="POST" id="add_user">
    <div class="new_item">
        <div class="form-group">
            <label for="name" class="text-light">Name</label>
            <input type="hidden" name="id" value="">
            <input type="text" name="name" value="" placeholder="US SE-1">
        </div>

        <div class="form-group">
            <label for="location" class="text-light">Location</label>
            <input type="text" name="location" value="" placeholder="Atlanta, Georgia">
        </div>

        <div class="form-group">
            <button type="submit" class="btn text-dark update">Save</button>
        </div>
    </div>
</form>